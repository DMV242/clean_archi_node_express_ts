
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Book {
  id        String   @id @default(uuid())
  title     String
  genre    String
  isAvailable Boolean
  author    String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}